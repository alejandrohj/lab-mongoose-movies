<h1>Movies</h1>
<a href="/movies/new">Create a new movie</a><br>
{{#each movies}}
  <a href="/movies/{{this._id}}">{{this.title}}</a><br>
  <form method="POST" action="/movies/{{this._id}}/delete">
    <button type="submit" class="btn btn-primary">Delete</button>
  </form>
  <form method="GET" action="/movies/{{this._id}}/edit">
    <button type="submit" class="btn btn-primary">Edit</button>
  </form>
{{/each}}